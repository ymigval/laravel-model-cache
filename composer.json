{
    "name": "ymigval/laravel-model-cache",
    "description": "Laravel package for caching Eloquent model queries",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Your Name",
            "email": "your.email@example.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/support": "^8.0|^9.0|^10.0",
        "illuminate/database": "^8.0|^9.0|^10.0",
        "illuminate/cache": "^8.0|^9.0|^10.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0|^7.0|^8.0",
        "phpunit/phpunit": "^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "Ymigval\\ModelCache\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ymigval\\ModelCache\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ymigval\\ModelCache\\ModelCacheServiceProvider"
            ]
        }
    }
}
