{
    "name": "ymigval/laravel-model-cache",
    "description": "Laravel package for caching Eloquent model queries",
    "homepage": "https://github.com/ymigval/laravel-model-cache",
    "keywords": [
        "redis",
        "php",
        "caching",
        "memcached",
        "laravel",
        "performance",
        "eloquent",
        "orm",
        "model",
        "cache",
        "laravel-package",
        "database-caching",
        "laravel-eloquent",
        "query-cacher",
        "model-cache"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Yordan",
            "email": "banatube@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
      "php": "^7.4|^8.0|^8.1|^8.2|^8.3",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/database": "^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/cache": "^8.0|^9.0|^10.0|^11.0|^12.0",
      "ext-json": "*"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0|^7.0|^8.0|^9.0|^10.0",
        "phpunit/phpunit": "^9.0|^10.0|^11.0",
        "friendsofphp/php-cs-fixer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "YMigVal\\LaravelModelCache\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "YMigVal\\LaravelModelCache\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "check-style": "vendor/bin/php-cs-fixer fix --dry-run",
        "fix-style": "vendor/bin/php-cs-fixer fix"
    },
    "extra": {
        "laravel": {
            "providers": [
                "YMigVal\\LaravelModelCache\\ModelCacheServiceProvider"
            ]
        }
    }
}
